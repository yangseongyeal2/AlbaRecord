    <TextView
        android:id="@+id/diligence_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="근면성:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/carrer"
        app:layout_constraintTop_toBottomOf="@+id/careerthing_text" />

    <TextView
        android:id="@+id/diligence"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="근면성정보"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/diligence_text"
        app:layout_constraintStart_toEndOf="@+id/diligence_text"
        app:layout_constraintTop_toTopOf="@+id/diligence_text" />
    <TextView
        android:id="@+id/flexibility_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="상황대처능력:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/carrer"
        app:layout_constraintTop_toBottomOf="@+id/diligence_text" />
    <TextView
        android:id="@+id/flexibility"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="상황대처능력정보"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/flexibility_text"
        app:layout_constraintStart_toEndOf="@+id/flexibility_text"
        app:layout_constraintTop_toTopOf="@+id/flexibility_text" />
    <TextView
        android:id="@+id/mastery_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="업무습득력:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/carrer"
        app:layout_constraintTop_toBottomOf="@+id/flexibility_text" />
    <TextView
        android:id="@+id/mastery"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="업무습득력정보"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/mastery_text"
        app:layout_constraintStart_toEndOf="@+id/mastery_text"
        app:layout_constraintTop_toTopOf="@+id/mastery_text" />
    <TextView
        android:id="@+id/attitude_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="근무태도:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/carrer"
        app:layout_constraintTop_toBottomOf="@+id/mastery_text" />
    <TextView
        android:id="@+id/attitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="업무습득력정보"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/attitude_text"
        app:layout_constraintStart_toEndOf="@+id/attitude_text"
        app:layout_constraintTop_toTopOf="@+id/attitude_text" />
    <TextView
        android:id="@+id/communication_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="커뮤니케이션:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/carrer"
        app:layout_constraintTop_toBottomOf="@+id/attitude_text" />
    <TextView
        android:id="@+id/communication"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="업무습득력정보"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/communication_text"
        app:layout_constraintStart_toEndOf="@+id/communication_text"
        app:layout_constraintTop_toTopOf="@+id/communication_text" />




    <TextView
        android:id="@+id/detail_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/edge_textview"
        android:text="자세한 사항"
        android:textAlignment="center"
        android:textColor="@color/colorBlack"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/communication_text" />
    <TextView
        android:id="@+id/SelfIntroduce"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="자세한 사항"
        android:textSize="20sp"
        android:textColor="@color/colorBlack"
        android:textStyle="bold"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detail_text" />
